 row = 1;\nvar currentRow = row;\nvar rowDistance = 7;\nvar aDistance = 0;\nvar aSpeed = 1;\nvar aDirection = 1;\n\/\/sizes\nvar aWidth = 40; \nvar aHeight = 40\n\n\/\/boss\nvar b1X = 300; \/\/b1 for boss #1\nvar b1Y = 100;\nvar bSpeed = 5;\nvar bDirection = 1;\nvar b1Width = 100;\nvar b1Height = 30;\nvar br1X = b1X; \/\/br1 for boss rocket #1\nvar br1Y = b1Y; \nvar br1Position = 1;\/\/keep track of where the boss rocket is\n\n\/\/rocket\nvar fire = false; \/\/am I firing a rocket?\nvar rSpeed = 5;\nvar rWidth = 7;\nvar rHeight = 20;\nvar r1X = p1X; \/\/r1 for rocket #1\nvar r1Y = p1Y; \nvar r1Position = 0;\/\/keep track of where the rocket is\n\n\/\/meteors\nvar m1X = 100; \/\/m1 for meteor #1\nvar m1Y = 400;\nvar m1Size = 50; \/\/square so one size\nvar m2X = 300; \/\/m2 for meteor #2\nvar m2Y = 400;\nvar m2Size = 50; \/\/square so one size\nvar m3X = 500; \/\/m3 for meteor #3\nvar m3Y = 400;\nvar m3Size = 50; \/\/square so one size\n\n\/\/counters\nvar stage = 0; \/\/determines stages of game (when to run functions)\nvar step = 0; \/\/for animated aliens\nvar score = 0;\nvar lives = 3;\nvar bossLife = 100;\nvar totalTime;\nvar splashTime;\nvar gameTime;\nvar timeLimit;\n\n\n\/\/media\n\/\/images\nvar playerImage;\nvar alienImage;\nvar alienImage2;\nvar bossImage;\nvar meteorImage;\n\/\/sounds\nvar explosion; \/\/alien hit\nvar blaster; \/\/player rocket fired\nvar lifelost; \/\/player loses life\nvar victory; \/\/run win function\nvar gameOver; \/\/run lose function\nvar backgroundMusic;\nvar start; \/\/run game gunction\n\/\/fonts\nvar titleFont;\nvar bodyFont;\n\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/SETUP\nfunction setup() {\n\tcreateCanvas(600, 500);\n\t\n\t\/\/modes\n\trectMode(CENTER);\n\timageMode(CENTER);\n\ttextAlign(CENTER);\n\t\n\t\/\/background music\n\t\tbackgroundMusic.play();\n\n\t\n}\/\/\/\/close setup\n\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/draw\nfunction draw() {\n\/\/timer\n\ttotalTime = millis();\/\/start clock\n\t\n\/\/set stages to run different game functions\n\tif(stage == 0){\n\t\tsplash();\n\t}\/\/close 0\n\t\n\tif(stage == 1){\n\t\tgame();\n\t}\/\/close 0\n\t\n\tif(stage == 2){\n\t\twin();\n\t}\/\/close 0\n\t\n\tif(stage == 3){\n\t\tlose();\n\t}\/\/close 0\n\t\n\/\/exit splash on mouse press:\n\tif(mouseIsPressed == true){\n\t\tstage = 1;\n\t\tstart.play();\n\t}\/\/close click so change stage\n\t\n}\/\/close draw\n\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/splash\nfunction splash() {\n\/\/timer\n\tsplashTime = totalTime;\n\t\n\/\/splash appearance\n\tbackground(0);\n\tstroke(0,255,0);\/\/green\n\tnoFill();\n\tstrokeWeight(3);\n\trect(width\/2, height\/2, width, height); \/\/border\n\t\n\/\/text\n\tnoStroke();\n\tfill(0,255,0);\/\/green\n\ttextSize(40);\n\ttextFont(titleFont);\n\ttext('SPACE INVADERS', width\/2, 100);\n\ttextSize(15);\n\ttextFont(bodyFont);\n\ttext('PROGRAMMED BY JASON ERDREICH, 2020', width\/2, 130);\n\ttextSize(40);\n\ttextFont(titleFont);\n\ttext('HOW TO PLAY', width\/2, 250);\n\ttextSize(15);\n\ttextFont(bodyFont);\n\ttext('PRESS LEFT AND RIGHT ARROWS TO MOVE', width\/2, 290);\n\ttext('PRESS S TO FIRE ROCKETS', width\/2, 320);\n\ttext('WATCH OUT FOR BOSS ROCKETS', width\/2, 350);\n\ttext('DESTROY ALL ALIENS TO WIN', width\/2, 380);\n\ttext('CLICK THE SCREEN TO BEGIN', width\/2, 450);\n\n\t\n\t\n}\/\/close splash\n\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/win\nfunction win() {\n\/\/win appearance\n\tbackground(0,255,0);\/\/green\n\tstroke(0,0,255);\/\/blue\n\tnoFill();\n\tstrokeWeight(3);\n\trect(width\/2, height\/2, width, height); \/\/border\n\t\n\/\/text\n\tnoStroke();\n\tfill(0,0,255);\/\/blue\n\ttextSize(60);\n\ttextFont(titleFont);\n\ttext('VICTORY', width\/2, 180);\n\ttextSize(20);\n\ttextFont(bodyFont);\n\ttext('YOUR TIME WAS', width\/2, 260);\n\ttext(gameTime, width\/2-60, 300);\n\ttext('SECONDS', width\/2+60, 300);\n\ttext('REFRESH TO PLAY AGAIN', width\/2, 450);\n\t\n\t\n}\/\/close splash\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/lose\nfunction lose() {\n\/\/lose appearance\n\tbackground(255,0,0);\/\/red\n\tstroke(0);\/\/black\n\tnoFill();\n\tstrokeWeight(3);\n\trect(width\/2, height\/2, width, height); \/\/border\n\t\n\/\/text\n\tnoStroke();\n\tfill(0);\/\/black\n\ttextSize(60);\n\ttextFont(titleFont);\n\ttext('GAME OVER', width\/2, 180);\n\ttextSize(20);\n\ttextFont(bodyFont);\n\ttext('YOU LIVED FOR', width\/2, 260);\n\ttext(gameTime, width\/2-60, 300);\n\ttext('SECONDS', width\/2+60, 300);\n\ttext('REFRESH TO TRY AGAIN', width\/2, 450);\n\t\n\t\n}\/\/close lose\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/game\nfunction game() {\t\n\/\/looping functions\n\tkeyPressed();\n\tkeyTyped();\n\t\n\/\/game appearance\n\tbackground(0);\n\tstroke(0,255,0);\/\/green\n\tnoFill();\n\tstrokeWeight(3);\n\trect(width\/2, height\/2, width, height); \/\/border\n\n\t\n\/\/draw aliens\n\taliens(); \/\/run alien function\n\n\/\/rocket function\n\tfill(26, 175, 255);\/\/light blue\n\tnoStroke();\n\trockets();\n\t\n\/\/draw player\n\tnoStroke();\n\tfill(0,0,255);\/\/blue\n\t\/\/rect(p1X, p1Y, pWidth, pHeight);\n\timage(playerImage, p1X, p1Y, pWidth, pHeight);\n\t\n\/\/bad guy function\n\tboss();\n\t\n\/\/meteor functions\n\tmeteors();\n\t\n\/\/timer function\n\tsplashTime = splashTime;\n\tgameTime = int((totalTime-splashTime)\/1000);\n\t\n\/\/counter display\n\tfill(0,255,0);\/\/green\n\trect(width\/2, 25, width, 50);\/\/banner\n\t\/\/game title\n\ttextSize(23);\n\ttextFont(titleFont);\n\tfill(0,0,255);\/\/blue\n\ttext('SPACE INVADERS', width\/2, 35);\n\tfill(0);\/\/black\n\t\/\/scoreboard\n\ttextSize(20);\n\ttextFont(titleFont);\n\ttext('Score:', 60, 45);\n\ttextSize(15);\n\ttextFont(bodyFont);\n\ttext(score, 125, 45);\n\t\/\/lives\n\ttextSize(20);\n\ttextFont(titleFont);\n\ttext('Lives:', 60, 25);\n\ttextSize(15);\n\ttextFont(bodyFont);\n\ttext(lives, 120, 25);\n\t\/\/lives\n\ttextSize(20);\n\ttextFont(titleFont);\n\ttext('TIME:', 510, 35);\n\ttextSize(15);\n\ttextFont(bodyFont);\n\ttext(gameTime, 570, 35);\n\t\n\/\/counter functions\n\tif(score >= 28){\n\t\tbackgroundMusic.stop();\n\t\tvictory.play();\n\t\tgameTime = gameTime; \/\/stop timer\n\t\tstage = 2; \/\/run win function\n\t}\/\/close win\n\t\n\tif(lives <= 0){\n\t\tbackgroundMusic.stop();\n\t\tgameOver.play();\n\t\tgameTime = gameTime; \/\/stop timer\n\t\tstage = 3; \/\/run lose function\n\t}\/\/close lose\n\n}\/\/\/close GAME\n\n\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/boss\nfunction boss(){\n\/\/draw boss\n\tfill(255,0,0);\/\/red\n\timage(bossImage, b1X, b1Y, b1Width, b1Height);\n\t\/\/boss health\n\ttextFont(bodyFont);\n\ttextSize(10);\n\ttext(bossLife, b1X, b1Y-20);\n\t\n\/\/move boss back and forth\n\tb1X = b1X + (bSpeed*bDirection);\n\tif(b1X >= width){\n\t\tbDirection = bDirection*-1;\/\/change direction\n\t}\/\/close > w\n\t\n\tif(b1X <= 0){\n\t\tbDirection = bDirection*-1;\/\/change direction\n\t}\/\/close < 0\n\t\n\/\/collisions with boss\n\t\/\/with b1\n\tif(r1X >= b1X-b1Width\/2 && r1X <= b1X+b1Width\/2 && r1Y >= b1Y-b1Height\/2 && r1Y <= b1Y+b1Height\/2){\n\t\t\/\/rocket hit boss\n\t\tif(bossLife >= 10){\n\t\t\t\/\/boss is still there\n\t\t\tscore = score+1; \/\/add point\n\t\t\tbossLife = bossLife-10;\n\t\t\texplosion.play();\n\t\t\tr1Position = 2;\/\/send rocket to return position\n\t\t} \/\/close hit boss\n\t\telse{\n\t\t\tb1X = -10000; \/\/move boss off screen\n\t\t\tbSpeed = 0;\n\t\t\tr1Position = 2;\/\/send rocket to return position\n\t\t}\/\/close else no more boss\n\t}\/\/close collision with boss\n\t\n\/\/BOSS rocket positions\n\t\/\/1 = in motion after firing\n\t\/\/2 = reset back to boss\n\t\n\/\/draw boss rocket\n\tfill(255,0,0);\/\/red\n\trect(br1X, br1Y, rWidth, rHeight);\n\t\n\/\/\/firing boss rockets\n\t\/\/boss rocket 1\n\tif(br1Position == 1){ \/\/position is 1 so fire boss rocket 1\n\t\tbr1X = br1X; \/\/stop following boss\n\t\tbr1Y = br1Y+rSpeed \/\/fire rocket\n\t\t\n\t\t\/\/when rocket exceeds window send back\n\t\t\tif(br1Y >= height ){\n\t\t\t\tbr1Position = 2;\n\t\t\t}\/\/close send back \n\t\n\t}\/\/close fire\n\telse{\n\t\tbr1Y = b1Y; \/\/not fired so stay with ship\n\t\tbr1X = b1X;\n\t}\/\/close else dont fire\n\t\n\t\/\/send rocket back on #2 command\n\tif(br1Position == 2){\n\t\tbr1Y = b1Y; \/\/put rocket back with boss\n\t\tbr1X = b1X;\n\t\tbr1Position = 1; \/\/reset position and fire again\n\t}\/\/close send back\n\t\n\/\/collisions with player\n\t\/\/with br1 with p1\n\tif(br1X >= p1X-pWidth\/2 && br1X <= p1X+pWidth\/2 && br1Y >= p1Y-pHeight\/2 && br1Y <= p1Y+pHeight\/2){\n\t\t\/\/boss rocket hit player\n\t\tlives = lives-1; \/\/lose life\n\t\tlifelost.play();\n\t\tp1X = width\/2;\/\/move player to center\n\t\tbr1Position = 2;\/\/send rocket to return position\n\t}\/\/close collision with player\n\t\n}\/\/close boss\n\n\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/aliens\nfunction aliens(){\n\t\/\/draw ANIMATED aliens\n\t\/\/\/start step counter\n\tstep = step+1;\n\t\n\tif(step == 0){\n\t\t\/\/row 1\n\t\timage(alienImage, a1X, a1Y, aWidth, aHeight); \/\/alien 1\n\t\timage(alienImage, a2X, a2Y, aWidth, aHeight); \/\/alien 2\n\t\timage(alienImage, a3X, a3Y, aWidth, aHeight); \/\/alien 3\n\t\timage(alienImage, a4X, a4Y, aWidth, aHeight); \/\/alien 4\n\t\timage(alienImage, a5X, a5Y, aWidth, aHeight); \/\/alien 5\n\t\timage(alienImage, a6X, a6Y, aWidth, aHeight); \/\/alien 6\n\t\timage(alienImage, a7X, a7Y, aWidth, aHeight); \/\/alien 7\n\t\timage(alienImage, a8X, a8Y, aWidth, aHeight); \/\/alien 8\n\t\timage(alienImage, a9X, a9Y, aWidth, aHeight); \/\/alien 9\n\t\t\/\/row 2\n\t\timage(alienImage, a10X, a10Y, aWidth, aHeight); \/\/alien 10\n\t\timage(alienImage, a11X, a11Y, aWidth, aHeight); \/\/alie