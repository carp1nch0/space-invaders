					<div :class="{'icon icon_play':1,'icon_restart': app.sketchPlaying,'rotate360': app.sketchRestarting, selected: app.currentMode == 'playMode' }" data-target="#sketch" v-tippy="{'distance':15}" :content="$t('sketch.play') + ` (${app.keyboardShortcuts.play.description})`" @click="playSketch()"></div>
					<div :class="{'icon icon_code':1, selected: app.currentMode == 'codeMode'}" data-target="#codePanel" v-tippy="{'distance':15}" :content="$t('sketch.code') + ` (${app.keyboardShortcuts.code.description})`" @click="app.currentMode = 'codeMode'"></div>
				</div>
