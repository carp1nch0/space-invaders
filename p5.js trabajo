								<div v-for="tag in sketch.tags" class="tag" v-bind:key="tag"><a :href="'/browse/?q=' + tag + '&time=anytime&type=tags'">{{tag}}</a></div>
							</li>

						</div>
					</ul>
				</div>
			</div>
		</div>
