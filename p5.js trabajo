					<div id="sideButtons" :class="{ showSideButtons }">

						<!-- Approve Button -->
						<op-button v-if="sketch.approveTo" id="approveButton" :button-class="{'btn btn-primary':1, 'btn-success': app.justApproved, 'loading': app.approving}" @click="approveSketch()" :extra="{[$t('general.reject')]: rejectSketch}">{{$t('general.approve')}}</op-button>

						<!-- Owner Fork button -->
						<button v-if="sessionUser.isOwner && sketch.visualID > 0" id="forkSketchLink" class="btn btn-link hidden-xs" @click="forkSketch">{{$t('general.saveAsFork')}}</button>

						<!-- Guest Fork button -->
						<button v-else-if="sessionUser.canInteractWith" v-show="sketch.isDirty" id="forkSketchButton" class="btn btn-primary hidden-xs" @click="forkSketch">{{$t('general.saveAsFork')}}</button>

						<!-- SaveSketchButton is only displayed on /create -->
						<button v-if="sessionUser.isOwner && sketch.visualID < 0" id="saveSketchButton" class="btn btn-primary" v-show="app.currentMode != 'editMode'  && (app.saving || sketch.isDirty)" v-text="$t('general.save')" @click="app.currentMode = 'editMode'" @click="saveSketch(false)"></button>

						<!-- Submit Button on Form -->
						<button id="submitSketchButton" :class="{ 'btn btn-primary': 1,  'btn-success': app.justSaved, 'loading': app.saving}" v-text="$t('general.submit')" form="editSketchForm" type="submit"></button>

						<!-- Quick Save on existing -->
						<button v-if="sessionUser.isOwner && sketch.visualID > 0" id="quickSaveButton" :class="{ 'btn btn-primary': 1, 'btn-success': app.justSaved, 'loading': app.saving}" v-text="$t('general.save')" @click="saveSketch(true)"></button>
					</div>
					<div id="infoPanelButtons">
						<button v-if="sessionUser.isOwner" id="editSketchButton" class="btn btn-primary" @click="app.currentMode = 'editMode'">{{$t('general.edit')}}</button>
					</div>
